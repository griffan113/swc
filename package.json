{
  "name": "swc",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:build": "npm run build && node --inspect dist/shared/infra/http/server.js",
    "dev:server": "cross-env PRODUCTION=false nodemon --delay 0 --ext ts --exec \"npm run dev:build\"",
    "build": "swc src -d dist",
    "server": "cross-env PRODUCTION=true node dist/shared/infra/http/server.js",
    "test": "jest",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src/**/*.ts"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint src/**/*.ts --fix"
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.40",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "prisma": "^3.15.2",
    "rate-limiter-flexible": "^2.3.7",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "tsyringe": "^4.7.0",
    "uuid": "^8.3.2"
  }
}
